{{> userHeader}}
<div class="bookings" >
  {{#each bookings}}
    <div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">name:{{this.name}}</h5>
    <p class="card-text">Booking status:{{this.status}}</p>
      <p class="card-text">Booking date:{{this.BookingDate}}</p>
    {{!-- <a href="#" class="btn btn-primary">Go somewhere</a> --}}
  </div>
</div>
 {{/each}}
</div>
<div class="container">
    <div class="card mb-3" style="max-width:50%;">
  <div class="row g-0  ">
      {{#each result}}
    <div class="col-md-4 cart"  style="margin-top: 10px;">
      <img src="/images/doctor/{{this.photo}}" class="img-fluid rounded-start" alt="...">
    </div>
    <div class="col-md-8 cart"  style="margin-top: 10px;">
      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
           <p class="card-text">{{this.time}}</p>
         <p class="card-text">Clinic name: <b>{{this.clinicName}}</b> </p>
          <p class="card-text">Clinic Place: <b>{{this.place}}</b> </p>
        {{!-- <h6 class="card-title">Quantity:{{this.qnty}}</h6> --}}
        <a href="/users/bookNow/{{this.name}}/{{this.clinicID}}" class="btn btn-outline-warning">Book Now</a>
     
{{!--         
        <a class="btn btn-primary" href="/users/qnty_decrement/{{this.Id}}" role="button">-</a>
        <a class="btn btn-primary" href="/users/qnty_increment/{{this.Id}}" role="button">+</a>
        <a class="btn btn-warning" href="/users/buynow/{{this.id}}/{{this.Price}}/{{this.qnty}}" role="button">Buy Now</a>
        <a class="btn btn-danger" href="/users/remove/{{this.Id}}" role="button">Remove</a> --}}

      </div>
    </div>
   
    {{/each}}
  </div>
</div>
</div>





